{
  "name": "join-non-empty-array",
  "version": "1.1.1",
  "description": "Join non-empty array elements into a string",
  "license": "MIT",
  "repository": "Mottie/join-non-empty-array",
  "author": {
    "name": "Rob Garrison",
    "email": "wowmotty@gmail.com"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "xo && ava && tsd"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "keywords": [
    "join",
    "array",
    "non-empty",
    "string"
  ],
  "xo": {
    "space": false,
    "rules": {
      "no-sparse-arrays": 0,
      "quotes": [
        "error",
        "double"
      ],
      "padding-line-between-statements": 0,
      "operator-linebreak": 0
    }
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "tsd": "^0.10.0",
    "xo": "^0.25.3"
  },
  "dependencies": {
    "array.prototype.flat": "^1.2.2"
  }
}
